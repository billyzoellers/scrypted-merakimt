         "Settings"
      ]
   },
   "dependencies": {
      "@types/node": "^16.6.1",
      "async-mqtt": "^2.6.2",
      "axios": "^0.27.2"
   },
   "devDependencies": {
      "@scrypted/sdk": "^0.0.198",
      "@typescript-eslint/eslint-plugin": "^5.22.0",
      "@typescript-eslint/parser": "^5.22.0",
      "eslint": "^8.16.0",
      "eslint-config-airbnb-base": "^15.0.0",
      "eslint-plugin-import": "^2.26.0",
      "webpack-bundle-analyzer": "^4.5.0"
   }
}
